{
  "name": "react-daterange",
  "version": "0.0.2",
  "description": "Daterange picker in react",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "esnext": "./esnext/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "jest --no-cache",
    "coverage": "jest --no-cache --coverage",
    "build": "npm run build:esm && npm run build:cjs && npm run build:esnext",
    "start": "node examples/server.js",
    "build:esm": "rimraf ./esm && tsc --target es5 --module es6 --outDir ./esm",
    "build:cjs": "rimraf ./lib && tsc --target es5 --module commonjs --outDir ./lib",
    "build:esnext": "rimraf ./esnext && tsc --target es6 --module es6 --outDir ./esnext",
    "prepublishOnly": "npm test && npm run build"
  },
  "files": [
    "esm",
    "lib",
    "esnext",
    "typings.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkbansal/react-daterange.git"
  },
  "keywords": [
    "date",
    "date-range",
    "date-picker",
    "react",
    "react-component",
    "glamorous"
  ],
  "author": "Vivek Kumar Bansal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vkbansal/react-daterange/issues"
  },
  "homepage": "https://github.com/vkbansal/react-daterange#readme",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.8",
    "@types/lodash.defaults": "^4.2.3",
    "@types/lodash.pick": "^4.4.3",
    "@types/lodash.range": "^3.2.3",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "@types/shallowequal": "^0.2.1",
    "cross-env": "^5.1.1",
    "date-fns": "^2.0.0-alpha.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "html-webpack-plugin": "^2.30.1",
    "illuminate-js": "^1.0.0-alpha.0",
    "jest": "^21.2.1",
    "jest-glamor-react": "^3.2.2",
    "jsdom": "^11.5.1",
    "prettier": "^1.9.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-docgen-typescript": "^1.1.0",
    "react-dom": "^16.2.0",
    "react-illuminate": "^2.0.0-alpha.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "snarkdown": "^1.2.2",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "shallowequal": "^1.0.2"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
